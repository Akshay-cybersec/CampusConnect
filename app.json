{
  "expo": {
    "name": "CampusConnect",
    "slug": "CampusConnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/applogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C353C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-build-properties"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/applogo.png",
        "backgroundColor": "#0C353C"
      },
      "package": "com.akshaycybersec.CampusConnect"
    },
    "web": {
      "favicon": "./images/applogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "91c65081-af6a-47dc-9228-0a72efafae7b"
      }
    }
  }
}
